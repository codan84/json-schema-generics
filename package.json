{
  "name": "json-schema-generics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "one-of-event:qt": "quicktype -s schema ./schemas/one-of-event/event.json -o ./__generated_types__/one-of-event/quicktype-types.ts --just-types",
    "one-of-event:j2t": "json2ts --no-additionalProperties --no-enableConstEnums --ignoreMinAndMaxItems -i ./schemas/one-of-event/event.json -o ./__generated_types__/one-of-event/json2ts-types.ts",
    "dynamic-ref:qt": "quicktype -s schema ./schemas/dynamic-ref-event/draft-event.json -o ./__generated_types__/dynamic-ref/quicktype-draft-event.ts --just-types",
    "dynamic-ref:j2t": "npx json2ts --no-additionalProperties --no-enableConstEnums --ignoreMinAndMaxItems -i ./schemas/dynamic-ref-event/draft-event.json -o ./__generated_types__/dynamic-ref/json2ts-draft-event.ts",
    "copy-paste:qt": "quicktype -s schema ./schemas/copy-paste-event/draft-event.json -o ./__generated_types__/copy-paste/quicktype-draft-event.ts --just-types",
    "copy-paste:j2t": "json2ts --no-additionalProperties --no-enableConstEnums --ignoreMinAndMaxItems -i ./schemas/copy-paste-event/draft-event.json -o ./__generated_types__/copy-paste/json2ts-draft-event.ts",
    "all-of:qt": "quicktype -s schema ./schemas/all-of-event/draft-event.json -o ./__generated_types__/all-of/quicktype-draft-event.ts --just-types",
    "all-of:j2t": "json2ts --no-additionalProperties --no-enableConstEnums --ignoreMinAndMaxItems -i ./schemas/all-of-event/draft-event.json -o ./__generated_types__/all-of/json2ts-draft-event.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.5.4",
    "json-schema-to-typescript": "^13.1.2",
    "jsonschema": "^1.4.1",
    "quicktype": "^23.0.107"
  }
}
